import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int c=s.nextInt();
        if (a>b && a>c){
            System.out.printf("a is largest then b and c");
        }else if (b>a && b>c){
            System.out.printf("b is largest then a and c");
        }else{
            System.out.printf("c is largest then a and b");
        }
        
    }
}
