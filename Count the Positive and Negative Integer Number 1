import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner s=new Scanner(System.in);
        int n =s.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=s.nextInt();
        }
        int p=0;
        int ne=0;
        for(int i=0;i<n;i++){
            if(arr[i]>0){
                p++;
            }else{
                ne++;
            }
        }
        System.out.printf("Count of Positive Integer is %.2f\n",(float)p);
        System.out.printf("Count of Negative Integer is %.2f\n",(float)ne);
            
    }
}
