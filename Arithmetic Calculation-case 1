import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        char x=s.next().charAt(0);
        double c;
        switch(x){
            case '+':
                c=a+b;
                System.out.printf("Addition of two number is %.1f\n",c);
                break;
                
            case '-':
                c=a-b;
                System.out.printf("Subtraction of two number is %.1f\n",c);
                break;
                
            case '*':
                c=a*b;
                System.out.printf("Multiplication of two number is %.1f\n",c);
                break;
                
            case '/':
                c=a/b;
                System.out.printf("Division of two number is %.1f\n",c);
                break;
                
            case '%':
                c=a%b;
                System.out.printf("Modulo of two number is %.1f\n",c);
                break;
                
            default:
                System.out.printf("Invalid Input");
        }
    }
}
